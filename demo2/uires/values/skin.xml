<?xml version="1.0" encoding="utf-8"?>

<skin>
  <imglist name="skin_girl"	src="img:png_girl" alpha="150"/>
  <imglist name="skin_love"	src="img:png_love"/>
  <imglist name="skin_soui"	src="img:png_soui"/>
  <imglist name="skin_tree_icon" src="img:png_treeicon" states="3" />
  <imglist name="skin_btn_expand" src="img:btn_expand" states="6">
    <states>
      <state index="0" value="normal"/>
      <state index="1" value="hover"/>
      <state index="2" value="hover|pushdown"/>
      <state index="3" value="normal|checked"/>
      <state index="4" value="hover|checked"/>
      <state index="5" value="hover|pushdown|checked"/>
    </states>
  </imglist>
</skin>
